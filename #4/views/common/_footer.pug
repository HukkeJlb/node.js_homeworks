footer.footer
  .container.footer__container
      .footer__auth 
        if(!loggedIn)
          a(href="/login").title-name Авторизация
            span.title-name__doter .
        else
          a(href="/admin").title-name Админ-панель
            span.title-name__doter .
      .footer__social
        ul.social
          each item in social
            li.social__item
              a.social__link(href="#")
                i(class=`fab ${item}`)